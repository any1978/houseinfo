<h1>物件情報一覧</h1>
<p><%= notice %></p>
<table class="table table-striped">
  <tr>
    <th class = "subject">物件名</th>
    <th class = "subject">賃料</th>
    <th class = "subject">住所</th>
    <th class = "subject">築年数</th>   
    <th class = "subject">備考欄</th>
    <%# <th colspan="3"></th> %>
  </tr>
<% @houseinfos.each do |houseinfo| %>
  <tr>
    <td><%= houseinfo.name %></td>
    <td><%= houseinfo.rent %>円</td>
    <td><%= houseinfo.address %></td>
    <td><%= houseinfo.age %>年</td>
    <td><%= houseinfo.note %></td>
    <td ><%= link_to 'View the details', houseinfo_path(houseinfo.id) %></td>
  </tr>
<% end %>
</table>
<%= link_to 'Post new', new_houseinfo_path, class:'btn' %>